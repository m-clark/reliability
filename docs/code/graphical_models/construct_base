 X1  [label = "X1"];
 X2  [label = "X2"];
 X3  [label = "X3"];
 X4  [label = "X4"];
 X5  [label = "X5"];

 node [shape=ellipse width=1];
   ? -> X1  ;
   ? -> X2  ;
   ? -> X3  ;
   ? -> X4  ;
   ? -> X5  ;
 
 { rank=same; 
 X1; X2; X3; X4; X5; }
 
}